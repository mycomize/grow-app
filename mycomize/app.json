{
  "expo": {
    "name": "mycomize",
    "slug": "mycomize",
    "version": "1.0.0",
    "scheme": "mycomize",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mycomize"
    },
    "android": {
      "architectures": ["arm64-v8a", "armeabi-v7a"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.anonymous.mycomize"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aaa8278e-1f4f-44c4-a8dd-06c577a70e53"
      }
    },
    "owner": "connojd"
  }
}
